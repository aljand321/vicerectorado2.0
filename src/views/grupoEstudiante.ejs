  <div class="container">
          <div class="row">
            <div class="col-md-5 offset-md-3">
              <div class="card">
                <div class="card-body">
                  <form class="" action="/ADDagEST" method="POST">
                    <div class="form-group">
                      <!-- <input type="text" name="numResolucion" class="form-control" value="Carrera" > -->
                      <select class="form-control" name="carrera">
                        <% for(var i=0; i < carrera.length; i++) { %>
								      	<option value="<%=carrera[i].nombre %>"><%=carrera[i].nombre %></option>
                          <% } %>
                      </select>
                    </div>

                    <div class="form-group">
                      <select class="form-control" name="gestion">
                        <option value="2018">2018</option>
                        <option value="2017">2017</option>
                        <option value="2016">2016</option>
                      </select>

                    </div>

                    <div class="form-group">
                      <select class="form-control" name="periodo">
                        <option value="gestion">Gestion</option>
                        <option value="1">1</option>
                        <option value="2">2</option>
                      </select>

                    </div>

                    <div class="form-group">
                      <select class="form-control" name="tipoBeca">
                        <option value="gestion">Auxiliar</option>
                        <option value="1">Trabajo</option>
                        <option value="2">Graduacion</option>
                      </select>

                    </div>

                    <button type="submit" class="btn btn-primary btn-block">Buscar<Datos></Datos></button>

                  </form>
                </div>
              </div>
            </div>

          </div>
        </div>

 <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
